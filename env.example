# TLEF GRASP Environment Variables
# Copy this file to .env and update the values for your environment

# Server Configuration
TLEF_GRASP_PORT=8070
SESSION_SECRET=your-secret-key-here

# Ollama Configuration
OLLAMA_HOST=localhost
OLLAMA_PORT=11434
OLLAMA_MODEL=llama3.2:latest

# Qdrant Configuration
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION_NAME=question-generation-collection

# UBC GenAI Toolkit Configuration
# These are typically handled automatically, but can be overridden if needed
EMBEDDINGS_MODEL=fast-bge-small-en-v1.5
VECTOR_SIZE=384
DISTANCE_METRIC=Cosine

# Debug Configuration
DEBUG=true
NODE_ENV=production
